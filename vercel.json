{
  "version": 2,
  "builds": [
    {
      "src": "src/server/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/server/**"]
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/sitemap.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "/sitemap",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/api/credits/(.*)",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/webhook",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/xendit-webhook",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/health",
      "dest": "/src/server/server.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/robots\\.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "/googlecb559eeeff31b59b\\.html",
      "dest": "/googlecb559eeeff31b59b.html"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|xml|txt))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
